---
import CarrouselComponents from "../carrouselComponents.astro";
import ProyectosAMedida1 from "@/assets/proyectos-a-medida/carrousel-proyectos-a-medida/proyectos-a-medida-1.webp";
import ProyectosAMedida2 from "@/assets/proyectos-a-medida/carrousel-proyectos-a-medida/proyectos-a-medida-2.webp";
import ProyectosAMedida3 from "@/assets/proyectos-a-medida/carrousel-proyectos-a-medida/proyectos-a-medida-3.webp";
import ProyectosAMedida4 from "@/assets/proyectos-a-medida/carrousel-proyectos-a-medida/proyectos-a-medida-4.webp";
import ProyectosAMedida5 from "@/assets/proyectos-a-medida/carrousel-proyectos-a-medida/proyectos-a-medida-5.webp";

const items = [
  { src: ProyectosAMedida1, alt: "Proyectos a medida 1" },
  { src: ProyectosAMedida2, alt: "Proyectos a medida 2" },
  { src: ProyectosAMedida3, alt: "Proyectos a medida 3" },
  { src: ProyectosAMedida4, alt: "Proyectos a medida 4" },
  { src: ProyectosAMedida5, alt: "Proyectos a medida 5" },
];
---

<CarrouselComponents
  items={items}
  outerClass="relative overflow-hidden  mt-10"
  containerClass="relative overflow-hidden "
  width="100%"
  aspectRatio="16/9"
  interval={3000}
  showControls={false}
  initialIndex={0}
/>
