---
import {Image} from "astro:assets";
import NativaImage from "@/assets/modelos/nativa/nativa-2.webp"

---

<section class="w-full h-full flex py-20 px-40 justify-between">
  <article class="flex flex-col p-10 gap-5">
    <div class="flex flex-col items-start w-[385px]">
      <h2 class="text-4xl text-gray-700 font-bold ">Falta texto
    </div>
    <div>
      <p class="text-md pt-2 leading-[1.7] text-justify text-gray-700 w-[400px] tracking-wide">
        Falta texto
      </p>
    </div>
    
  </article>
  <aside class="w-[800px] h-">
    <Image src={NativaImage} alt="Habitat" class="w-full aspect-[16/9] object-fill"/>
  </aside>
</section>
