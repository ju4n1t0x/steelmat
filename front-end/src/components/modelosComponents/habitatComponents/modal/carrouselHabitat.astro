---
import Carrousel from "@/components/carrouselComponents.astro";
import Habitat1 from "@/assets/modelos/habitat/habitat-1.webp";
import Habitat2 from "@/assets/modelos/habitat/habitat-2.webp";
import Habitat3 from "@/assets/modelos/habitat/habitat-3.webp";

const items = [
  { src: Habitat1, alt: "Habitat 1 - render" },
  { src: Habitat2, alt: "Habitat 2 - render" },
  { src: Habitat3, alt: "Habitat 3 - render" }
];
---

<Carrousel
  items={items}
  carouselId="habitat-1-1"
  outerClass="relative overflow-hidden rounded-lg mt-5 "
  containerClass="relative overflow-hidden rounded-lg object-cover"
  width="100%"
  aspectRatio="16/9"
  interval={0}
  showControls={true}
  initialIndex={0}
  imgClass="object-fill"
  
/>

