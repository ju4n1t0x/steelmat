---
import Essentia1 from "@/assets/modelos/essentia/essentia-1.webp";
import Essentia2 from "@/assets/modelos/essentia/carrousel/essentia-1.1/essentia-1.1-2.webp";
import Essentia3 from "@/assets/modelos/essentia/carrousel/essentia-1.1/essentia-1.1-3.webp";
import Essentia4 from "@/assets/modelos/essentia/carrousel/essentia-1.1/essentia-1.1-4.webp";
import Essentia5 from "@/assets/modelos/essentia/carrousel/essentia-1.1/essentia-1.1-5.webp";
import Carrousel from "@/components/carrouselComponents.astro";

const items = [
  {
    src: Essentia1,
    alt: "Essentia 1",
  },
  {
    src: Essentia2,
    alt: "Essentia 2",
  },
  {
    src: Essentia3,
    alt: "Essentia 3",
  },
  {
    src: Essentia4,
    alt: "Essentia 4",
  },
  {
    src: Essentia5,
    alt: "Essentia 5",
  },
];
---

<Carrousel
  items={items}
  outerClass="relative overflow-hidden  mt-10"
  containerClass="relative overflow-hidden "
  width="100%"
  aspectRatio="16/9"
  interval={3000}
  showControls={false}
  initialIndex={0}
  imgClass="object-fill"
/>
