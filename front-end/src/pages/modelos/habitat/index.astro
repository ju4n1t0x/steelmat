---
import MainLayout from "@/layouts/MainLayout.astro";
import HeroHabitat from "@/components/modelosComponents/habitatComponents/heroHabitat.astro";
import DescripcionHabitat from "@/components/modelosComponents/habitatComponents/descripcionHabitat.astro";
import RenderHabitat from "@/components/modelosComponents/habitatComponents/renderHabitat.astro";
import Navegacion from "@/components/modelosComponents/navegacionComponents.astro";
---

<MainLayout
  title="Modelo Habitat | Casa Steel Frame 115m² 3 Dormitorios | SteelMat"
  description="Casa steel frame Habitat de 115m² con 3 dormitorios en Santa Fe. Diseño funcional, construcción rápida y terminaciones de calidad. Ideal para familias. ¡Conocé el modelo!"
  navBarTextColor="text-gray-700"
  iconUrl="/icons/steelmat-negro.webp"
  textHoverColor="gray-700"
>
  <HeroHabitat />
  <DescripcionHabitat />
  <RenderHabitat />
  <Navegacion
    hrefVolver="/modelos/"
    hrefSiguiente="/modelos/essentia"
    textSiguiente="Ver Essentia siguiente"
  />
</MainLayout>
